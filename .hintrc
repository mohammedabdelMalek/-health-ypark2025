{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "background-color: -internal-light-dark(white, #3b3b3b)",
          "-webkit-rtl-ordering",
          "border-color: -internal-light-dark(#767676, #858585)",
          "offset-rotate",
          "zoom",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "css-prefix-order": "off",
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off",
        "aria-required-parent": "off",
        "aria-command-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ]
  }
}